<?xml version="1.0" encoding="UTF-8"?>
<ValidationRule xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>OriginOrProductMustBeFilled</fullName>
    <active>true</active>
    <errorConditionFormula>AND( ISNULL(Origin__c ) ,  ISNULL(Product_Factcode__c ))</errorConditionFormula>
    <errorMessage>Both Origin And Product Fact Code can&apos;t be Empty.</errorMessage>
</ValidationRule>
