<?xml version="1.0" encoding="UTF-8"?>
<ValidationRule xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>CWT_Rebate_Percent</fullName>
    <active>true</active>
    <errorConditionFormula>IF(AND(
ISPICKVAL(CWT_Rebate_Y_N__c,&apos;Yes&apos;),
ISBLANK(CWT_Rebate_Percent__c)
),true,false)</errorConditionFormula>
    <errorDisplayField>CWT_Rebate_Percent__c</errorDisplayField>
    <errorMessage>Value Required if &apos;CWT Rebate&apos; is set to &apos;Yes&apos;</errorMessage>
</ValidationRule>
